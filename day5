a = input("Do you like 'hanging around'?: ")
if a.lower() == 'no':
  print('Then you are not a spider-man')
else:
  print('Then you are a spider-man')
print()
b = input('Do you like rotten food?: ')
if b.lower() == 'yes':
  print('Then you are a vulture')
else:
  print('Then you are not a vulture')
print()
c = input('Do you like sighing?: ')
if c.lower() == 'yes':
  print('Then you are a bird')
else:
  print('Then you are not a bird')
  

